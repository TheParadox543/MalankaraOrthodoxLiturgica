[
    {
        "id": 1,
        "type": "prose",
        "lit_en": "",
        "lit_ml": "പട്ടക്കാരൻ:- ശുബഹോ",
        "lit_mn": "",
        "special": ""
    },
    {
        "id": 2,
        "type": "prose",
        "lit_en": "",
        "lit_ml": "ജനം:- ബലഹീനരും പാപികളുമായ ഞങ്ങളുടെമേൽ അനുഗ്രഹങ്ങളും കരുണയും രണ്ടുലോകങ്ങളിലും എന്നുമെന്നേക്കും ചൊരിയപ്പെടുമാറാകണമേ. ആമ്മീൻ.",
        "lit_mn": "",
        "special": ""
    },
    {
        "id": 3,
        "type": "subheading",
        "lit_en": "",
        "lit_ml": "സ്കൂസൊ",
        "lit_mn": "",
        "special": ""
    },
    {
        "id": 4,
        "type": "subheading",
        "lit_en": "",
        "lit_ml": "(ദൈവമേ! നിന്റെ കൃപ പോലെ എന്നോടു.. പേജ്..)",
        "lit_mn": "",
        "special": ""
    },
    {
        "id": 5,
        "type": "subheading",
        "lit_en": "",
        "lit_ml": "(എന്റെ ദൈവമേ! നീ എന്റെ ദൈവമാകുന്നു;.. പേജ്..)",
        "lit_mn": "",
        "special": ""
    },
    {
        "id": 6,
        "type": "subheading",
        "lit_en": "",
        "lit_ml": "എനിയോനോ",
        "lit_mn": "",
        "special": ""
    },
    {
        "id": 7,
        "type": "subheading",
        "lit_en": "",
        "lit_ml": "(ഹൌദ്ഉഹദോനൊ)",
        "lit_mn": "",
        "special": ""
    },
    {
        "id": 8,
        "type": "song",
        "lit_en": "",
        "lit_ml": "/t നിൻമുൻ വന്നേൻ- കൃപ ചെയ്തുന്മേൽ.",
