[
    {
        "type": "heading",
        "content": "Ooshaanapperunnaallil kurbbaanaanubhavam"
    },
    {
        "type": "subheading",
        "content": "(samrakshithajaathikalle ... Enna pole)"
    },
    {
        "type": "song",
        "content": "Garddabhavaa-hanane! Nal-kompukallenti\nKuttikall keertthichavane-mashihaa! Sthothram,"
    },
    {
        "type": "song",
        "content": "Thelliyuka see-yonmakalle! Thava nrpanaarnnu\nVaathil thura-nnettuka shama-daayakane nee"
    },
    {
        "type": "song",
        "content": "Pithrsahitham-vaanil sthuthi-elkkunnone\nBhoomithale-baalajanam-vaazhttheedunnu."
    },
    {
        "type": "song",
        "content": "Svaprabhayaal-sraappikalle-anchikkunnon\nSeeyonil-garhithamaam-garddabhameri."
    },
    {
        "type": "song",
        "content": "Vinayamezhum-praanapriya-premaaveshaal\nSthuthigeetha-prakaratthaa-lavane vaazhtthi."
    },
    {
        "type": "song",
        "content": "Mrthiyaamirull-neekki than-suthare kaattha\nVishvadyuthi-ye! Shaantyaa-vannaalum nee."
    },
    {
        "type": "song",
        "content": "Mama makkall-kkaishvaryam-nalkunnone!\nDhanashaalee! Karunaabdhe!-vannaalum nee."
    },
    {
        "type": "song",
        "content": "Thiru rudhiraal-rakshippaa-nange vitta\nDhanyapithaa-vinnuyara-ngallilooshaanaa."
    },
    {
        "type": "song",
        "content": "Panineerppoo-parimallane! Premaakaaraa!\nJeevaushadha-saurabhyam-nee veeshunnu."
    },
    {
        "type": "song",
        "content": "Minneedu-nnagnidyuthi-ninnil ninnum,\nVasthram jee-vanumuththhaa-navumekunnu."
    },
    {
        "type": "song",
        "content": "Andhatha me-nalkiya saa-tthaan lajichu\nSvargeeya-prabhayenne-bhaasippichu."
    },
    {
        "type": "song",
        "content": "Thirumarmma-ngalle rooshmaa-cheythallo nyaan,\nMama paapam maayikkuka-krpayaal naathhaa!"
    }
]