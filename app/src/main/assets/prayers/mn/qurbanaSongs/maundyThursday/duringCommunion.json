[
    {
        "type": "heading",
        "content": "Vi. Kurbbaana kodukkumpoll"
    },
    {
        "type": "subheading",
        "content": "(rosalee rosalee)"
    },
    {
        "type": "song",
        "content": "Rahasyam rahasyam-kalpicheeshan\nRahasyamenikkum -enveettaarkkum\n/tsvargeeyanaaya -manavaallaa! The,\n/tsthuthiyennu nyangall-ghoshikkunnu."
    },
    {
        "type": "song",
        "content": "Mrthiye kedutthee-ttaadaaminu nal-\nPraanan koduttha - gaathravumithu thaan."
    },
    {
        "type": "song",
        "content": "Samudrodaratthe-balatthaal pakutthu\nAditthattil maargam-thellichonivan thaan."
    },
    {
        "type": "song",
        "content": "Eereyabhojyam-svargeeyamannaa -\nMaruvil manujan-bhujichathumithu thaan."
    },
    {
        "type": "song",
        "content": "Kolaykkaayu nayikka-ppettorajamaayu\nEshaaya kanda-darshanamithu thaan."
    },
    {
        "type": "song",
        "content": "Nokkippaarkkum-naallil noha-\nPothatthil modaal-kandathumithu thaan."
    },
    {
        "type": "song",
        "content": "Prathhamam prathya-kshathayin rahasye\nNaathhan niyamicha-naallumithu thaan."
    },
    {
        "type": "song",
        "content": "Ajatthe vadhichu ripuvil ninnum\nKidaye vidarttha-perunnaallithu thaan."
    },
    {
        "type": "song",
        "content": "Inne divasam-harmye mashihaa\nShishyar sahitham-pesahaa bhujichu."
    },
    {
        "type": "song",
        "content": "Yaakkobusutharkk-maruvin naduvil\nKudippaan koduttha-shailavumithu thaan."
    },
    {
        "type": "song",
        "content": "Seenaayu malamel-vannangirangi\nMeghasthambhe-vasichonivan thaan."
    },
    {
        "type": "song",
        "content": "Ee perunnaallil-jeevanagaathram\nVibhajichavane narare sthuthippin."
    },
    {
        "type": "song",
        "content": "Medatthileesha-thanayan hathanaayu\nMedam thannil-pesahaa ajavum."
    },
    {
        "type": "song",
        "content": "Bhoovaasikalle sutharakthatthaal\nViduvichavane - sthuthi paadiduvin."
    }
]