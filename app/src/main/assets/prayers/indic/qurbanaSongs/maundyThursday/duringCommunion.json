[
    {
        "type": "heading",
        "content": "vi. kuṟbbāna koṭukkumpōḷ"
    },
    {
        "type": "subheading",
        "content": "(ṟosalī ṟosalī)"
    },
    {
        "type": "song",
        "content": "rahasyaṁ rahasyaṁ-kal‌piccīśaṉ\nrahasyamenikkuṁ -eṉvīṭṭāṟkkuṁ\n/tsvaṟggīyanāya -maṇavāḷā! tē,\n/ts‌tutiyenn ñaṅṅaḷ-ghōṣikkunnu."
    },
    {
        "type": "song",
        "content": "mr̥tiye keṭuttī-ṭṭādāminu nal-\nprāṇaṉ koṭutta - gātravumitu tāṉ."
    },
    {
        "type": "song",
        "content": "samudrōdaratte-balattāl pakutt\naṭittaṭṭil māṟggaṁ-teḷiccōnivaṉ tāṉ."
    },
    {
        "type": "song",
        "content": "īṟēyabhōjyaṁ-svaṟggīyamannā -\nmaruvil manujaṉ-bhujiccatumitu tāṉ."
    },
    {
        "type": "song",
        "content": "kolaykkāy nayikka-ppeṭṭōrajamāy\nēśāya kaṇṭa-daṟśanamitu tāṉ."
    },
    {
        "type": "song",
        "content": "nōkkippāṟkkuṁ-nāḷil nōha-\npōtattil mōdāl-kaṇṭatumitu tāṉ."
    },
    {
        "type": "song",
        "content": "prathamaṁ pratya-kṣatayiṉ rahasyē\nnāthaṉ niyamicca-nāḷumitu tāṉ."
    },
    {
        "type": "song",
        "content": "ajatte vadhicc ripuvil ninnuṁ\nkiṭaye viṭaṟtta-perunnāḷitu tāṉ."
    },
    {
        "type": "song",
        "content": "innē divasaṁ-haṟmyē maśihā\nśiṣyaṟ sahitaṁ-pesahā bhujiccu."
    },
    {
        "type": "song",
        "content": "yākkōbusutaṟkk-maruviṉ naṭuvil\nkuṭippāṉ koṭutta-śailavumitu tāṉ."
    },
    {
        "type": "song",
        "content": "sīnāy malamēl-vannaṅṅiṟaṅṅi\nmēghastaṁbhē-vasiccōnivaṉ tāṉ."
    },
    {
        "type": "song",
        "content": "ī perunnāḷil-jīvanagātraṁ\nvibhajiccavane nararē s‌tutippiṉ."
    },
    {
        "type": "song",
        "content": "mēṭattilīśa-tanayaṉ hatanāy\nmēṭaṁ tannil-pesahā ajavuṁ."
    },
    {
        "type": "song",
        "content": "bhūvāsikaḷe sutaraktattāl\nviṭuviccavane - s‌tuti pāṭiṭuviṉ."
    }
]