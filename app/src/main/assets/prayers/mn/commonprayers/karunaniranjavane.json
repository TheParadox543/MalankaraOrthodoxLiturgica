[
    {
        "type": "heading",
        "content": "Bovooso"
    },
    {
        "type": "subheading",
        "content": "(maar baalaayi)"
    },
    {
        "type": "song",
        "content": "Karunaniranyavane! Punaruththhaanatthil\nNinnude srshdiye nee-puthuthaakkeedaname."
    },
    {
        "type": "song",
        "content": "Ninnil nidrayada-nyaagamanam kaakkum\nMrtharil shaantiyana-chekuka parihaaram."
    },
    {
        "type": "song",
        "content": "Ninnabhaye mrthare-abraahaamisahaakku\nYaakkobennivar than-madiyil paarppikka"
    },
    {
        "type": "song",
        "content": "Vannavanaam varuvon-mrthareyettunnon\nSuthanennaarttheedum meyyodaathmaavum."
    }
]